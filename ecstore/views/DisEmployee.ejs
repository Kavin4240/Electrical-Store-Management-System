<!DOCTYPE html>
<html>
<head>
<title>Manage Employees</title>
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha2/dist/css/bootstrap.min.css" rel="stylesheet"  >
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha2/dist/js/bootstrap.bundle.min.js" ></script>
<style>
a
{
  text-decoration: none;
  font-family: 'Times New Roman', Times, serif;
  color: white;
}
.nav-link
{
   font-size:20px;
}
body{
  
  font-family: 'Times New Roman', Times, serif;
}
h1,h5,p{
  font-family: 'Times New Roman', Times, serif;
}
</style>

</head>
<body>
<form><div class="container-fluid text-center">
<div><h2>Employee List</h2></div>

<table class="table table-bordered">
  <tr class="table-success">
    <th>Ename</th>
    <th>Email</th>
    <th>Experience</th>
    <th>Phone number</th>
    <th>Salary</th>
    <th>Position</th>
    <th>Address</th>
    <th>Edit</th>
    <th>Delete</th>
  </tr>
<%for(let i of empinfo){%>
<tr class="table-success">
  <td><%=i.ename %></td>
   <td><%=i.email %></td> 
  <td><%=i.exp %></td>
  <td ><%=i.phno %></td>
  <td><%=i.sal %></td>
  <td><%=i.pos %></td>
  <td><%=i.add %></td>
 <td> <button class="btn btn-success"><a href="/empinfo/<%=i._id %>/edits">Edit</a></button></td>
<td><button class="btn btn-danger"><a href="/empinfo/<%=i._id %>/delete">Delete</a></button></td></form>
</div>
</tr>
<%}%>
</table>
</div>
</body>
</html>