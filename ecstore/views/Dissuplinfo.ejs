 <!-- <td> <button class="btn btn-success"><a href="/empinfo/<%=i._id %>/edits">Edit</a></button></td> -->
<td><button class="btn btn-danger"><a href="/supinfo/<%=i._id %>/delete">Delete</a></button></td></form>
</div>
</tr>
